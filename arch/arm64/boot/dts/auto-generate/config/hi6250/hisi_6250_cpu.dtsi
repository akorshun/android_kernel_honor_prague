

  /{
		cpus {
		#address-cells = <2>;
		#size-cells = <0>;

		little0: cpu@0 {
			device_type = "cpu";
			compatible = "arm,cortex-a53", "arm,armv8";
			reg = <0x0 0x0>;
			clocks = <&clk_cluster0 0>;
			clock-names = "cpu-cluster.0";
			enable-method = "psci";
			clock-frequency = <0>;
			clock-latency = <0>;
			cpu-idle-states = <&CPU_SLEEP_0 &CLUSTER_SLEEP_0>;
			sched-energy-costs = <&CPU_COST_A53 &CLUSTER_COST_A53>;
			operating-points-v2 = <&cluster0_opp_table>;
			operating-points = <
				480000	700000
				807000	800000
				1306000	900000
				1709000	1000000
			>;
		};
		little1:cpu@1 {
			device_type = "cpu";
			compatible = "arm,cortex-a53", "arm,armv8";
			reg = <0x0 0x1>;
			clocks = <&clk_cluster0 0>;
			clock-names = "cpu-cluster.0";
			enable-method = "psci";
			clock-frequency = <0>;
			clock-latency = <0>;
			cpu-idle-states = <&CPU_SLEEP_0 &CLUSTER_SLEEP_0>;
			sched-energy-costs = <&CPU_COST_A53 &CLUSTER_COST_A53>;
			operating-points-v2 = <&cluster0_opp_table>;
			operating-points = <
				480000	700000
				807000	800000
				1306000	900000
				1709000	1000000
			>;
		};
		little2:cpu@2 {
			device_type = "cpu";
			compatible = "arm,cortex-a53", "arm,armv8";
			reg = <0x0 0x2>;
			clocks = <&clk_cluster0 0>;
			clock-names = "cpu-cluster.0";
			enable-method = "psci";
			clock-frequency = <0>;
			clock-latency = <0>;
			cpu-idle-states = <&CPU_SLEEP_0 &CLUSTER_SLEEP_0>;
			sched-energy-costs = <&CPU_COST_A53 &CLUSTER_COST_A53>;
			operating-points-v2 = <&cluster0_opp_table>;
			operating-points = <
				480000	700000
				807000	800000
				1306000	900000
				1709000	1000000
			>;
		};
		little3:cpu@3 {
			device_type = "cpu";
			compatible = "arm,cortex-a53", "arm,armv8";
			reg = <0x0 0x3>;
			clocks = <&clk_cluster0 0>;
			clock-names = "cpu-cluster.0";
			enable-method = "psci";
			clock-frequency = <0>;
			clock-latency = <0>;
			cpu-idle-states = <&CPU_SLEEP_0 &CLUSTER_SLEEP_0>;
			sched-energy-costs = <&CPU_COST_A53 &CLUSTER_COST_A53>;
			operating-points-v2 = <&cluster0_opp_table>;
			operating-points = <
				480000	700000
				807000	800000
				1306000	900000
				1709000	1000000
			>;
		};

		big2:cpu@102 {
			device_type = "cpu";
			compatible = "arm,cortex-a5x", "arm,armv8";
			reg = <0x0 0x102>;
			clocks = <&clk_cluster1 0>;
			clock-names = "cpu-cluster.1";
			enable-method = "psci";
			clock-frequency = <0>;
			clock-latency = <0>;
			cpu-idle-states = <&CPU_SLEEP_0 &CLUSTER_SLEEP_1>;
			sched-energy-costs = <&CPU_COST_A72 &CLUSTER_COST_A72>;
			operating-points-v2 = <&cluster1_opp_table>;
			operating-points = <
				1402000	800000
				1805000	900000
				2016000	1000000
				2112000	1000000
				2362000 1100000
			>;
		};
		big3:cpu@103 {
			device_type = "cpu";
			compatible = "arm,cortex-a5x", "arm,armv8";
			reg = <0x0 0x103>;
			clocks = <&clk_cluster1 0>;
			clock-names = "cpu-cluster.1";
			enable-method = "psci";
			clock-frequency = <0>;
			clock-latency = <0>;
			cpu-idle-states = <&CPU_SLEEP_0 &CLUSTER_SLEEP_1>;
			operating-points-v2 = <&cluster1_opp_table>;
			sched-energy-costs = <&CPU_COST_A72 &CLUSTER_COST_A72>;
			operating-points = <
				1402000	800000
				1805000	900000
				2016000	1000000
				2112000	1000000
				2362000 1100000
			>;
		};
		big0:cpu@100 {
			device_type = "cpu";
			compatible = "arm,cortex-a5x", "arm,armv8";
			reg = <0x0 0x100>;
			clocks = <&clk_cluster1 0>;
			clock-names = "cpu-cluster.1";
			enable-method = "psci";
			clock-frequency = <0>;
			clock-latency = <0>;
			cpu-idle-states = <&CPU_SLEEP_0 &CLUSTER_SLEEP_1>;
			operating-points-v2 = <&cluster1_opp_table>;
			sched-energy-costs = <&CPU_COST_A72 &CLUSTER_COST_A72>;
			operating-points = <
				1402000	800000
				1805000	900000
				2016000	1000000
				2112000	1000000
				2362000 1100000
			>;
		};
		big1:cpu@101 {
			device_type = "cpu";
			compatible = "arm,cortex-a5x", "arm,armv8";
			reg = <0x0 0x101>;
			clocks = <&clk_cluster1 0>;
			clock-names = "cpu-cluster.1";
			enable-method = "psci";
			clock-frequency = <0>;
			clock-latency = <0>;
			cpu-idle-states = <&CPU_SLEEP_0 &CLUSTER_SLEEP_1>;
			operating-points-v2 = <&cluster1_opp_table>;
			sched-energy-costs = <&CPU_COST_A72 &CLUSTER_COST_A72>;
			operating-points = <
				1402000	800000
				1805000	900000
				2016000	1000000
				2112000	1000000
				2362000 1100000
			>;
		};

		cluster0_opp_table: opp_table0 {
			compatible = "operating-points-v2";
			opp-shared;

			opp0 {
				opp-supported-hw = <0x7>;
				opp-hz = /bits/ 64 <480000000>;
				opp-microvolt = <700000>;
				clock-latency-ns = <500000>;
			};
			opp1 {
				opp-supported-hw = <0x7>;
				opp-hz = /bits/ 64 <807000000>;
				opp-microvolt = <800000>;
				clock-latency-ns = <500000>;
			};
			opp2 {
				opp-supported-hw = <0x7>;
				opp-hz = /bits/ 64 <1306000000>;
				opp-microvolt = <900000>;
				clock-latency-ns = <500000>;
			};
			opp3 {
				opp-supported-hw = <0x7>;
				opp-hz = /bits/ 64 <1709000000>;
				opp-microvolt = <1000000>;
				clock-latency-ns = <500000>;
			};
		};

		cluster1_opp_table: opp_table1 {
			compatible = "operating-points-v2";
			opp-shared;

			opp0 {
				opp-supported-hw = <0x7>;
				opp-hz = /bits/ 64 <1402000000>;
				opp-microvolt = <800000>;
				clock-latency-ns = <500000>;
			};
			opp1 {
				opp-supported-hw = <0x7>;
				opp-hz = /bits/ 64 <1805000000>;
				opp-microvolt = <900000>;
				clock-latency-ns = <500000>;
			};
			opp2 {
				opp-supported-hw = <0x7>;
				opp-hz = /bits/ 64 <2016000000>;
				opp-microvolt = <1000000>;
				clock-latency-ns = <500000>;
			};
			opp3 {
				opp-supported-hw = <0x7>;
				opp-hz = /bits/ 64 <2112000000>;
				opp-microvolt = <1000000>;
				clock-latency-ns = <500000>;
			};
			opp4 {
				opp-supported-hw = <0x6>;
				opp-hz = /bits/ 64 <2362000000>;
				opp-microvolt = <1100000>;
				clock-latency-ns = <500000>;
			};
		};

		idle-states {
			entry-method = "arm,psci";

			CPU_SLEEP_0: cpu-sleep-0 {
				compatible = "arm,idle-state";
				arm,psci-suspend-param = <0x0010000>;
				entry-latency-us = <40>;
				exit-latency-us = <70>;
				min-residency-us = <3000>;
				local-timer-stop;
			};

			CLUSTER_SLEEP_0: cluster-sleep-0 {
				compatible = "arm,idle-state";
				arm,psci-suspend-param = <0x1010000>;
				entry-latency-us = <500>;
				exit-latency-us = <5000>;
				min-residency-us = <12000>;
				local-timer-stop;
			};
			CLUSTER_SLEEP_1: cluster-sleep-1 {
				compatible = "arm,idle-state";
				arm,psci-suspend-param = <0x1010000>;
				entry-latency-us = <1000>;
				exit-latency-us = <5000>;
				min-residency-us = <10000>;
				local-timer-stop;
			};
		};
		cpu-map {
			cluster0: cluster0 {
				#cooling-cells = <3>; /* min followed by max */
				core0 {
					cpu = <&little0>;
				};
				core1 {
					cpu = <&little1>;
				};
				core2 {
					cpu = <&little2>;
				};
				core3 {
					cpu = <&little3>;
				};
			};
			cluster1: cluster1 {
				#cooling-cells = <3>; /* min followed by max */
				core0 {
					cpu = <&big0>;
				};
				core1 {
					cpu = <&big1>;
				};
				core2 {
					cpu = <&big2>;
				};
				core3 {
					cpu = <&big3>;
				};
			};
		};
		/include/ "hisi_6250_sched_energy.dtsi"
	};

	support-target {
		compatible = "hisi,supportedtarget";
		support_name = "Kirin 650","Kirin 655","Kirin 659";
		support_value = <2112000>, <2362000>, <2362000>;
		support_efuse = "false";
	};

	target-cpu {
		compatible = "hisi,targetcpu";
		target_cpu = "Kirin 650";
	};

	menu-switch {
		compatible = "hisi,menu-switch";
		cpu-mask = <0x0>, <0xF0>;
		switch-profile = <1402000>;
	};
  };
